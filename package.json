    "dist",
    "lib",
    "src"
  ],
  "keywords": [
    "react",
    "reactjs",
    "responsive",
    "media",
    "media-query",
    "match-media"
  ],
  "author": "Luis Merino Garcia <mail@luismerino.name> (http://github.com/Rendez)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Rendez/react-media-responsive/issues"
  },
  "homepage": "https://github.com/Rendez/react-media-responsive",
